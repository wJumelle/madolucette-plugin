{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "mel/faq-elem",
  "version": "0.0.1",
  "title": "MeL FAQ élément",
  "category": "common",
  "description": "Élément permettant de saisir une question et une réponse.",
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "question": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    },
    "reponse": {
      "type": "string",
      "source": "html",
      "selector": "div.mel-faq-elem--reponse"
    }
  },
  "textdomain": "melplugin",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}